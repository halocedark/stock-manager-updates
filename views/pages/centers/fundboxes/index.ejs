<script>

$(async function()
{

var page_container = $('#self_fundbox_container');
if ( page_container[0] == undefined )
    return;

var ERROR_BOX = page_container.find('#ERROR_BOX');

var deleteSelectedBTN = page_container.find('#deleteSelectedBTN');
var pagination = page_container.find('#pagination');
var table_element = page_container.find('#table_element');

var search_input = page_container.find('#search_input');
var search_btn = page_container.find('#search_btn');
var date_start_input = page_container.find('#date_start_input');
var date_end_input = page_container.find('#date_end_input');

var fundbox_total = page_container.find('#fundbox_total');

var fundbox_actions_extra_data_list = page_container.find('#fundbox_actions_extra_data_list')

var center_select = page_container.find('#center_select')
var distributor_select = page_container.find('#distributor_select')
var employee_select = page_container.find('#employee_select')
var client_select = page_container.find('#client_select')
// set primary dates
date_start_input.val(CURRENT_DATE);
date_end_input.val(CURRENT_DATE);
// Unconfirmed actions (owner)
var unconfirmed_fundbox_actions_pagination = page_container.find('#unconfirmed_fundbox_actions_pagination')
var unconfirmed_fundbox_actions_table_element = page_container.find('#unconfirmed_fundbox_actions_table_element')

var search_unconfirmed_fundbox_actions_btn = page_container.find('#search_unconfirmed_fundbox_actions_btn')
var search_unconfirmed_fundbox_actions_input = page_container.find('#search_unconfirmed_fundbox_actions_input')

var date_start_unconfirmed_fundbox_actions_input = page_container.find('#date_start_unconfirmed_fundbox_actions_input')
var date_end_unconfirmed_fundbox_actions_input = page_container.find('#date_end_unconfirmed_fundbox_actions_input')

var unconfirmed_fundbox_actions_fundbox_total = page_container.find('#unconfirmed_fundbox_actions_fundbox_total')
var unconfirmed_fundbox_actions_extra_data_list = page_container.find('#unconfirmed_fundbox_actions_extra_data_list')
var unconfirmed_fundbox_actions_confirm_button = page_container.find('#unconfirmed_fundbox_actions_confirm_button')
var unconfirmed_fundbox_actions_delete_button = page_container.find('#unconfirmed_fundbox_actions_delete_button')

var unconfirmed_center_select = page_container.find('#unconfirmed_center_select')
var unconfirmed_distributor_select = page_container.find('#unconfirmed_distributor_select')
var unconfirmed_employee_select = page_container.find('#unconfirmed_employee_select')
var unconfirmed_client_select = page_container.find('#unconfirmed_client_select')
// 
date_start_unconfirmed_fundbox_actions_input.val(CURRENT_DATE)
date_end_unconfirmed_fundbox_actions_input.val(CURRENT_DATE)
// Unconfirmed actions (source)
var unconfirmed_fundbox_actions_source_pagination = page_container.find('#unconfirmed_fundbox_actions_source_pagination')
var unconfirmed_fundbox_actions_source_table_element = page_container.find('#unconfirmed_fundbox_actions_source_table_element')

var search_unconfirmed_fundbox_actions_source_btn = page_container.find('#search_unconfirmed_fundbox_actions_source_btn')
var search_unconfirmed_fundbox_actions_source_input = page_container.find('#search_unconfirmed_fundbox_actions_source_input')

var date_start_unconfirmed_fundbox_actions_source_input = page_container.find('#date_start_unconfirmed_fundbox_actions_source_input')
var date_end_unconfirmed_fundbox_actions_source_input = page_container.find('#date_end_unconfirmed_fundbox_actions_source_input')

var unconfirmed_fundbox_actions_source_fundbox_total = page_container.find('#unconfirmed_fundbox_actions_source_fundbox_total')
var unconfirmed_fundbox_actions_source_extra_data_list = page_container.find('#unconfirmed_fundbox_actions_source_extra_data_list')
var unconfirmed_fundbox_actions_source_delete_button = page_container.find('#unconfirmed_fundbox_actions_source_delete_button')

var unconfirmed_source_center_select = page_container.find('#unconfirmed_source_center_select')
var unconfirmed_source_distributor_select = page_container.find('#unconfirmed_source_distributor_select')
var unconfirmed_source_employee_select = page_container.find('#unconfirmed_source_employee_select')
var unconfirmed_source_client_select = page_container.find('#unconfirmed_source_client_select')
// 
date_start_unconfirmed_fundbox_actions_source_input.val(CURRENT_DATE)
date_end_unconfirmed_fundbox_actions_source_input.val(CURRENT_DATE)

var owner_type = 'centers'

var res = await FUNDBOX_MODEL.actions_owner_show({
    owner_id: USER_CONFIG.administration.clinicId,
    owner_type: owner_type,
})
if ( res.code == 404 ) return
var FundBoxObject = res.data
// fundbox total
fundbox_total.text( moneyFormat(FundBoxObject.total) )
// delete selected
deleteSelectedBTN.on('click', async e =>
{
    PromptConfirmDialog().then(async c =>
    {
        deleteSelectedBTN.addClass('disabled')
        var response = await FUNDBOX_MODEL.actions_batchDelete({
            collection: selectedRows()
        });
        deleteSelectedBTN.removeClass('disabled')
        // hide loader
        TopLoader('', false);
        if ( response.code == 404 )
        {
            ERROR_BOX.show(0).delay(7*1000).hide(0).find('#text').text(response.message);
            return;
        }
        ERROR_BOX.show(0).delay(7*1000).hide(0).find('#text').text(response.message);
        //
        displayAll({
            method_type: 'actions_owner_search',
            owner_id: USER_CONFIG.administration.clinicId,
            owner_type: owner_type,
            query: search_input.val()
        })
    });
});
// search between dates
search_btn.on('click', async e =>
{
    displayAll({
        method_type: 'actions_owner_search',
        owner_id: USER_CONFIG.administration.clinicId,
        owner_type: owner_type,
        query: search_input.val(),
        advanced: {
            start: date_start_input.val(),
            end: date_end_input.val(),
        }
    })
})
// search
search_input.on('keyup', async e =>
{
    displayAll({
        method_type: 'actions_owner_search',
        owner_id: USER_CONFIG.administration.clinicId,
        owner_type: owner_type,
        query: search_input.val()
    })
})
// filter
center_select.on('change', async e =>
{
    displayAll({
        method_type: 'actions_owner_filterBySource',
        owner_id: USER_CONFIG.administration.clinicId,
        owner_type: owner_type,
        filter: center_select.find(':selected').val(),
    })
})
distributor_select.on('change', async e =>
{
    displayAll({
        method_type: 'actions_owner_filterBySource',
        owner_id: USER_CONFIG.administration.clinicId,
        owner_type: owner_type,
        filter: distributor_select.find(':selected').val(),
    })
})
employee_select.on('change', async e =>
{
    displayAll({
        method_type: 'actions_owner_filterBySource',
        owner_id: USER_CONFIG.administration.clinicId,
        owner_type: owner_type,
        filter: employee_select.find(':selected').val(),
    })
})
client_select.on('change', async e =>
{
    displayAll({
        method_type: 'actions_owner_filterBySource',
        owner_id: USER_CONFIG.administration.clinicId,
        owner_type: owner_type,
        filter: client_select.find(':selected').val(),
    })
})

// display all
displayAll({
    method_type: 'actions_owner_search',
    owner_id: USER_CONFIG.administration.clinicId,
    owner_type: owner_type,
    query: search_input.val()
})
async function displayAll(params)
{
    //
    table_element.find('.tbody').html('');
    fundbox_actions_extra_data_list.html('')
    SectionLoader(table_element)
    if ( params.method_type == 'actions_owner_search' )
        var res = await FUNDBOX_MODEL.actions_owner_search(params)
    else if ( params.method_type == 'actions_owner_filterBySource' )
        var res = await FUNDBOX_MODEL.actions_owner_filterBySource(params)

    SectionLoader(table_element, '')
    if ( res.code == 404 )
        return;
    
    var data = res.data;
    var html = '';
    var in_amount_total = 0
    var out_amount_total = 0
    var last_data = firstArrayElement(data)
    $.each(data, (k,v) =>
    {
        in_amount_total += parseFloat(v.in_amount)
        out_amount_total += parseFloat(v.out_amount)
        var _document = (v.document) ? `<a href="${v.document.url}" download="${path.basename(v.document.url)}">${FUI_DISPLAY_LANG.views.pages.global.download_file_btn}</a>` : ''
        html+= `<div class="tr" data-role="row" data-id="${v.id}">
                    <div class="td">
                        <input type="checkbox" class="form-check-input pointer" data-role="CHECK">
                    </div>
                    <div class="td">${v.owner_name}</div>
                    <div class="td">${v.reason}</div>
                    <div class="td">${v.note}</div>
                    <div class="td">
                        (<span class="text-success">${ (v.in_amount > 0) ? '+'+moneyFormat(v.in_amount) : '0.00' }</span>)
                    </div>
                    <div class="td">
                        (<span class="text-danger">${ (v.out_amount > 0) ? '-'+moneyFormat(v.out_amount) : '0.00' }</span>)
                    </div>
                    <div class="td">${v.source_name}</div>
                    <div class="td pointer">${_document}</div>
                    <div class="td">${v.created_at}</div>
                </div>PAG_SEP`
    })
    // add html
    new SmoothPagination(pagination, table_element.find('.tbody'), {
        data: html.split('PAG_SEP'),
        resultsPerPage: 12
    });
    // display extra data
    in_amount_total = parseFloat(in_amount_total).toFixed(2)
    out_amount_total = parseFloat(out_amount_total).toFixed(2)
    var last_amount = (last_data.in_amount > 0) ? `(<span class="text-success">+${moneyFormat(last_data.in_amount)}</span>)` : `(<span class="text-danger">-${moneyFormat(last_data.out_amount)}</span>)`
    fundbox_actions_extra_data_list.html(`

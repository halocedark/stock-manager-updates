<script>

    $(function () {

        const page_container = $('#show_customer_profile_container')

        if (!page_container[0]) return

        const ERROR_BOX = page_container.find('#ERROR_BOX')

        let CustomerObject = sessionData(true)
        // Card
        const customer_card_more_actions__menu = page_container.find('#customer_card_more_actions__menu')
        const customerName_element = page_container.find('#customerName_element')
        const customerPhone_element = page_container.find('#customerPhone_element')
        // Profile data
        const personal_details_tab = page_container.find('#personal_details_tab')

        personal_details_tab.on('tab:opened', e =>
        {
            const personal_details_tab__customerName_element = page_container.find('#personal_details_tab__customerName_element')
            const personal_details_tab__customerPhone_element = page_container.find('#personal_details_tab__customerPhone_element')
            const personal_details_tab__customerEmail_element = page_container.find('#personal_details_tab__customerEmail_element')
            const personal_details_tab__customerTotalDepst_element = page_container.find('#personal_details_tab__customerTotalDepst_element')

            personal_details_tab__customerName_element.text( CustomerObject.name )
            personal_details_tab__customerPhone_element.text( CustomerObject.phone )
            personal_details_tab__customerEmail_element.text( CustomerObject.email )
            personal_details_tab__customerTotalDepst_element.text( moneyFormat(CustomerObject.total_depts) )
        })
        personal_details_tab.trigger('tab:opened')

        // center_to_customer_invoices_panel
        const center_to_customer_invoices_panel = page_container.find('#center_to_customer_invoices_panel')
        // customer_dept_actions_panel
        const customer_dept_actions_panel = page_container.find('#customer_dept_actions_panel')

        // setup card
        setupCard()
        function setupCard()
        {
            // display info
            customerName_element.text(CustomerObject.name)
            customerPhone_element.text(CustomerObject.phone)
            // actions
            customer_card_more_actions__menu.off('click').on('click', e =>
            {
                const target = $(e.target)

                if ( target.data('role') == 'UPDATE' )
                {
                    CreateCustomerDialog__Centers(CustomerObject)
                }
            })
        }

        // Panels
        center_to_customer_invoices_panel.on('panel:opened', e =>
        {
            setupInvoices()
        })

        customer_dept_actions_panel.on('panel:opened', e =>
        {
            setupDeptActions()
        })
        // setup invoices
        function setupInvoices()
        {
            const center_to_customer_invoices_search_btn = page_container.find('#center_to_customer_invoices_search_btn')
            const center_to_customer_invoices_search_input = page_container.find('#center_to_customer_invoices_search_input')
            const center_to_customer_invoices_date_start_input = page_container.find('#center_to_customer_invoices_date_start_input')
            const center_to_customer_invoices_date_end_input = page_container.find('#center_to_customer_invoices_date_end_input')
            const center_to_customer_invoices_pagination = page_container.find('#center_to_customer_invoices_pagination')
            const center_to_customer_invoices_tableElement = page_container.find('#center_to_customer_invoices_tableElement')

            // center_to_customer_invoices_tableElement click
            center_to_customer_invoices_tableElement.on('click', async e => {
                var target = $(e.target);
                if (target[0].nodeName == 'TD') {
                    var parent = target.closest('tr');
                    var nextTR = parent.next();
                    nextTR.slideToggle(200)
                    parent.toggleClass('expanded');
                }

                if (target.data('role') == 'PRINT_INVOICE') {
                    e.preventDefault()
                    var parent = target.closest('[data-role="row"]')

                    parent.addClass('disabled')
                    var res = await ORDER_MODEL.show({
                        order_id: parent.data('id')
                    })
                    parent.removeClass('disabled')

                    if (res.code == 404) return

                    printInvoice({
                        data: res.data,
                        class: target.data('print-class'),
                    })
                }
            });
            
            // search
            center_to_customer_invoices_search_btn.on('click', e => {
                center_to_customer_invoices_search_input.trigger('keyup')
            })
            detectTypingEnd(center_to_customer_invoices_search_input, val => {
                displayCentralAdministrationProductsInvoices({
                    method_type: 'direction_center_to_customer_selling_search',
                    query: val,
                    advanced: {
                        start: center_to_customer_invoices_date_start_input.val(),
                        end: center_to_customer_invoices_date_end_input.val(),
                        user_id: CustomerObject.id,
                        supplier_id: USER_CONFIG.administration.clinicId,
                    }
                })
            })
            center_to_customer_invoices_search_input.trigger('keyup')
            // displayCentralAdministrationProductsInvoices
            async function displayCentralAdministrationProductsInvoices(params) {
                SectionLoader(center_to_customer_invoices_tableElement)

                if (params.method_type == 'direction_center_to_customer_selling_search') var res = await ORDER_MODEL.direction_center_to_customer_selling_search(params)

                SectionLoader(center_to_customer_invoices_tableElement, '')

                if (res.code == 404) return

                var data = res.data
                var html = ''

                for (var i = 0; i < data.length; i++) {
                    var order = data[i];
                    // loop items
                    if (order.order_items) {
                        var itemsHTML = '<ul class="list-group list-group-flush list-group-numbered">';
                        var itemsHTML2 = '';
                        var order_item_dept_rate = 0.00;
                        var order_item_price = 0.00;
                        for (var j = 0; j < order.order_items.length; j++) {
                            var item = order.order_items[j];
                            order_item_dept_rate += parseFloat(item.item_dept.order_item_dept_rate);
                            order_item_price += parseFloat(item.order_item_price);
                            itemsHTML += `<li class="list-group-item text-04">${item.item_name}</li>`;
                            itemsHTML2 += `<tr>
                                    <td>${item.item_name}</td>
                                    <td>${item.order_item_quantity}</td>
                                    <td>${moneyFormat(item.order_item_price)}</td>
                                    <td>${moneyFormat(item.order_item_final_amount)}</td>
                                    </tr>`;
                        }
                        itemsHTML += '</ul>';
                    }

                    html += `<tr class="expand-row" data-role="row" data-id="${order.order_id}">
                        <td>
                            <input type="checkbox" class="form-check-input pointer" data-role="CHECK" data-orderid="${order.order_id}">
                        </td>
                        <td>${order.order_hash}</td>
                        <td>${order.order_date}|${order.order_time}</td>
                        <td>${moneyFormat(order.order_total_amount)}</td>
                        <td>${moneyFormat(order.order_amount_paid)}</td>
                        <td>
                            <div class="py-2 px-2">
                                <a class="d-inline-block mx-2 text-dark" type="button" data-bs-toggle="dropdown" aria-expanded="false">
                                    ${FUI_DISPLAY_LANG.views.pages.global.more}
                                </a>
                                <ul class="dropdown-menu px-2">
                                    <li>
                                        <a href="#" class="no-link d-block py-2 border-bottom" data-role="PRINT_INVOICE" data-print-class="A3_PRINT" data-perm="a3_invoice_print">${FUI_DISPLAY_LANG.views.pages.global.print_sizes.A3_PRINT}</a>
                                    </li>
                                    <li>
                                        <a href="#" class="no-link d-block py-2 border-bottom" data-role="PRINT_INVOICE" data-print-class="A4_PRINT" data-perm="a4_invoice_print">${FUI_DISPLAY_LANG.views.pages.global.print_sizes.A4_PRINT}</a>
                                    </li>
                                    <li>
                                        <a href="#" class="no-link d-block py-2 border-bottom" data-role="PRINT_INVOICE" data-print-class="A5_PRINT" data-perm="a5_invoice_print">${FUI_DISPLAY_LANG.views.pages.global.print_sizes.A5_PRINT}</a>
                                    </li>
                                    <li>
                                        <a href="#" class="no-link" data-role="PRINT_INVOICE" data-print-class="RECEIPT_7_5_CM_PRINT" data-perm="receipt_7_5_cm_invoice_print">${FUI_DISPLAY_LANG.views.pages.global.print_sizes.RECEIPT_7_5_CM_PRINT}</a>
                                    </li>
                                </ul>
                            </div>
                        </td>
                        <td style="width:15px">
                            <svg part="svg" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" aria-labelledby="ic-caret-up" focusable="false" viewBox="0 0 24 24" class="expand-icon" dataV17f0a767=""><g><path fill-rule="evenodd" clip-rule="evenodd" d="M7.41 15.41L12 10.83L16.59 15.41L18 14L12 8L6 14L7.41 15.41Z"></path></g></svg>
                        </td>
                    </tr>PAG_SEP
                    <tr class="expand-row__row" style="display:none;">
                        <td colspan="10">
                            <table class="table-01">
                                <thead>
                                    <tr>
                                        <th>${FUI_DISPLAY_LANG.views.pages.global.th26}</th>
                                        <th>${FUI_DISPLAY_LANG.views.pages.global.th27}</th>
                                        <th>${FUI_DISPLAY_LANG.views.pages.global.th28}</th>
                                        <th>${FUI_DISPLAY_LANG.views.pages.global.th29}</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    ${itemsHTML2}
                                </tbody>
                            </table>
                            <div class="order-price-table">
                                <div class="order-price-table-contents">
                                    ${itemsHTML}	
                                </div>
                                <div class="order-price-table-contents">
                                    <div class="order-price-table-td">
                                        <strong>${FUI_DISPLAY_LANG.views.pages.global.th30}</strong>
                                        <span>${moneyFormat(order.order_total_amount)}</span>
                                    </div>
                                    <div class="order-price-table-td">
                                        <strong>${FUI_DISPLAY_LANG.views.pages.global.th31}</strong>
                                        <span>${moneyFormat(order.order_dept_amount)}</span>
                                    </div>
                                    <div class="border-top my-1"></div>
                                    <div class="order-price-table-td">
                                        <strong>${FUI_DISPLAY_LANG.views.pages.global.th32}</strong>
                                        <span>${moneyFormat(order.order_amount_paid)}</span>
                                    </div>	
                                </div>
                            </div>
                        </td>
                    </tr>PAG_SEP`;
                }
                // add html
                new SmoothPagination(center_to_customer_invoices_pagination, center_to_customer_invoices_tableElement.find('tbody'), {
                    data: html.split('PAG_SEP'),
                    resultsPerPage: 12
                });
            } 
        }
        // setup dept actions
        function setupDeptActions()
        {
            const customer_dept_actions_search_btn = page_container.find('#customer_dept_actions_search_btn')
            const customer_dept_actions_search_input = page_container.find('#customer_dept_actions_search_input')
            const customer_dept_actions_date_start_input = page_container.find('#customer_dept_actions_date_start_input')
            const customer_dept_actions_date_end_input = page_container.find('#customer_dept_actions_date_end_input')
            const customer_dept_actions_pagination = page_container.find('#customer_dept_actions_pagination')
            const customer_dept_actions_tableElement = page_container.find('#customer_dept_actions_tableElement')

            // customer_dept_actions_tableElement click
            customer_dept_actions_tableElement.on('click', async e => {
                var target = $(e.target);
                
            });
            
            // search
            customer_dept_actions_search_btn.on('click', e => {
                customer_dept_actions_search_input.trigger('keyup')
            })
            detectTypingEnd(customer_dept_actions_search_input, val => {
                displayCentralAdministrationDeptActions({
                    method_type: 'dept_action_search',
                    query: val,
                    advanced: {
                        start: customer_dept_actions_date_start_input.val(),
                        end: customer_dept_actions_date_end_input.val(),
                        customer_id: CustomerObject.id,
                        administration_id: USER_CONFIG.administration.clinicId,
                    }
                })
            })
            customer_dept_actions_search_input.trigger('keyup')
            // displayCentralAdministrationDeptActions
            async function displayCentralAdministrationDeptActions(params) {
                SectionLoader(customer_dept_actions_tableElement)

                if (params.method_type == 'dept_action_search') var res = await CUSTOMER_MODEL.dept_action_search(params)

                SectionLoader(customer_dept_actions_tableElement, '')

                if (res.code == 404) return

                var data = res.data
                var html = ''

                $.each(data, (k,v) =>
                {
                    html += `
                    <div class="tr" data-role="row" data-id="${v.id}">
                        <div class="td">
                            <input type="checkbox" class="form-check-input pointer" data-role="CHECK">
                        </div>
                        <div class="td">${v.reason}</div>
                        <div class="td">${v.note}</div>
                        <div class="td">
                            (<span class="text-success">${ (v.in_amount > 0) ? '+'+moneyFormat(v.in_amount) : '0.00' }</span>)
                        </div>
                        <div class="td">
                            (<span class="text-danger">${ (v.out_amount > 0) ? '-'+moneyFormat(v.out_amount) : '0.00' }</span>)
                        </div>
                        <div class="td">${v.created_at}</div>
                    </div>PAG_SEP
                    `
                })
                
                // add html
                new SmoothPagination(customer_dept_actions_pagination, customer_dept_actions_tableElement.find('.tbody'), {
                    data: html.split('PAG_SEP'),
                    resultsPerPage: 12
                });
            } 
        }
        

    })

</script>
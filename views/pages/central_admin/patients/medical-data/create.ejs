	const pelvic_circumferences_table_box_pptx_files_panel__trigger = page_container.find('#pelvic_circumferences_table_box_pptx_files_panel__trigger')

	const pelvic_circumferences_table_box_create_form = page_container.find('#pelvic_circumferences_table_box_create_form')
	const pelvic_circumferences_table_box_create_form__weight_value_input = page_container.find('#pelvic_circumferences_table_box_create_form__weight_value_input')
	const pelvic_circumferences_table_box_create_form__date_input = page_container.find('#pelvic_circumferences_table_box_create_form__date_input')
	const pelvic_circumferences_table_box_create_form__time_input = page_container.find('#pelvic_circumferences_table_box_create_form__time_input')
	const pelvic_circumferences_table_box_create_form__is_file_croppable_check = page_container.find('#pelvic_circumferences_table_box_create_form__is_file_croppable_check')
	const pelvic_circumferences_table_box_create_form__file_input = page_container.find('#pelvic_circumferences_table_box_create_form__file_input')
	const pelvic_circumferences_table_box_create_form__file_preview = page_container.find('#pelvic_circumferences_table_box_create_form__file_preview')

	var PelvicCircumferenceObject = new FormData()

	// submit
	pelvic_circumferences_table_box_create_form.off('submit').on('submit', async e =>
	{
		e.preventDefault()

		// check if value not empty when type is "report"

		if ( pelvic_circumferences_table_box_create_form__weight_value_input.val() == '' )
		{
			CreateToast('PS', FUI_DISPLAY_LANG.views.messages.error_empty_input, '')
			return
		}
		
		PelvicCircumferenceObject.append('clinicId', PatientObject.clinicId)
		PelvicCircumferenceObject.append('clinicName', PatientObject.clinicName)
		PelvicCircumferenceObject.append('patientId', PatientObject.patientId)
		PelvicCircumferenceObject.append('patientName', PatientObject.patientName)
		PelvicCircumferenceObject.append('value', pelvic_circumferences_table_box_create_form__weight_value_input.val())
		PelvicCircumferenceObject.append('date', pelvic_circumferences_table_box_create_form__date_input.val())
		PelvicCircumferenceObject.append('time', pelvic_circumferences_table_box_create_form__time_input.val())


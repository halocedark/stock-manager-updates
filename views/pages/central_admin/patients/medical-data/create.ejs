	// selected rows
	function weights_table_box_selectedRows()
	{
		var list = []
		var items = weights_table_box_table_element.find('[data-role="row"]')
		for (var i = 0; i < items.length; i++) 
		{
			var parent = $(items[i])
			var check = parent.find('[data-role="CHECK"]')
			if ( check.is(':checked') )
			{
				list.push({
					id: parent.data('id'),
				})
			}
		}

		return list
	}

	// Insert data
	const weights_table_box_pptx_reports_files_list = page_container.find('#weights_table_box_pptx_reports_files_list')
	const weights_table_box_pptx_files_panel__trigger = page_container.find('#weights_table_box_pptx_files_panel__trigger')

	const weights_table_box_create_form = page_container.find('#weights_table_box_create_form')
	const weights_table_box_create_form__weight_value_input = page_container.find('#weights_table_box_create_form__weight_value_input')
	const weights_table_box_create_form__weight_type_select = page_container.find('#weights_table_box_create_form__weight_type_select')
	const weights_table_box_create_form__date_input = page_container.find('#weights_table_box_create_form__date_input')
	const weights_table_box_create_form__time_input = page_container.find('#weights_table_box_create_form__time_input')
	const weights_table_box_create_form__is_file_croppable_check = page_container.find('#weights_table_box_create_form__is_file_croppable_check')
	const weights_table_box_create_form__file_input = page_container.find('#weights_table_box_create_form__file_input')
	const weights_table_box_create_form__file_preview = page_container.find('#weights_table_box_create_form__file_preview')

	var WeightObject = new FormData()

	// submit
	weights_table_box_create_form.off('submit').on('submit', async e =>
	{
		e.preventDefault()

		// check if value not empty when type is "report"

		if ( weights_table_box_create_form__weight_type_select.find(':selected').val() == 'report'
			&& weights_table_box_create_form__weight_value_input.val() == '' )
		{
			CreateToast('PS', FUI_DISPLAY_LANG.views.messages.error_empty_weight, '')
			return
		}
		
		WeightObject.append('clinicId', PatientObject.clinicId)
		WeightObject.append('clinicName', PatientObject.clinicName)
		WeightObject.append('patientId', PatientObject.patientId)
		WeightObject.append('patientName', PatientObject.patientName)
		WeightObject.append('value', weights_table_box_create_form__weight_value_input.val())
		WeightObject.append('type', weights_table_box_create_form__weight_type_select.find(':selected').val() )
		WeightObject.append('date', weights_table_box_create_form__date_input.val())
		WeightObject.append('time', weights_table_box_create_form__time_input.val())

		weights_table_box_create_form__file_input.next().removeClass('d-none')
		weights_table_box_create_form.find(':submit').addClass('disabled')

		// check if there are any pptx files selected
		const PPTX_FILES = await weights_table_box__selected_pptx_reports_files()

		if ( PPTX_FILES.length > 0 )
		{

			for (let i = 0; i < PPTX_FILES.length; i++) 
			{
				const PPTX_FILE = PPTX_FILES[i]

				// general
				// WeightObject.set('value', 0)
				// WeightObject.set('type', 'general')
				WeightObject.set('file', PPTX_FILE)

				var res = await PATIENT_MODEL.weight_store(WeightObject, progress =>
				{
					var percent = progress.percentComplete.toFixed(2)
					// 
					weights_table_box_create_form__file_input.next().find('.progress-bar').css('width', percent+'%').text(percent+'%')
				})

				CreateToast('PS', res.message, '')
				
			}

			weights_table_box_create_form__reset()
			setup_weights()

			return
		}

		// check if image cropped and upload cropped in "report" and original in "general"
		if ( weights_table_box_create_form__is_file_croppable_check.is(':checked')
			&& weights_table_box_create_form__weight_type_select.find(':selected').val() == 'report' )
		{
			// report
			var res = await PATIENT_MODEL.weight_store(WeightObject, progress =>
			{
				var percent = progress.percentComplete.toFixed(2)
				// 
				weights_table_box_create_form__file_input.next().find('.progress-bar').css('width', percent+'%').text(percent+'%')
			})

			if ( res.code == 404 ) return

			// general
			// WeightObject.set('value', 0)
			WeightObject.set('type', 'general')
			WeightObject.set('file', weights_table_box_create_form__file_input[0].files[0])

			var res = await PATIENT_MODEL.weight_store(WeightObject, progress =>
			{
				var percent = progress.percentComplete.toFixed(2)
				// 
				weights_table_box_create_form__file_input.next().find('.progress-bar').css('width', percent+'%').text(percent+'%')
			})

			if ( res.code == 404 ) return

			CreateToast('PS', res.message, '')

			weights_table_box_create_form__reset()
			setup_weights()

			return
		}
		// submit
		var res = await PATIENT_MODEL.weight_store(WeightObject, progress =>
		{
			var percent = progress.percentComplete.toFixed(2)
			// 
			weights_table_box_create_form__file_input.next().find('.progress-bar').css('width', percent+'%').text(percent+'%')
		})

		CreateToast('PS', res.message, '')
		
		if ( res.code == 404 ) return

		weights_table_box_create_form__reset()
		setup_weights()
	})
	// select file
	weights_table_box_create_form__file_input.off('file-selected').on('file-selected', e =>
	{
		var detail = e.originalEvent.detail

		if ( isMSPowerPointFile(detail.files[0].name) )
		{
			SectionLoader(weights_table_box_create_form__file_input.parent())

			weights_table_box_pptx_reports_files_list.html('')
			convertPptToImage(detail.files[0], (error, data, response) =>
			{
				SectionLoader(weights_table_box_create_form__file_input.parent(), '')
				
				if ( error ) 
				{
					console.error(error)
					return
				}
				var html = ''
				for (let i = 0; i < data.PngResultPages.length; i++) 
				{
					const image = data.PngResultPages[i];
					const uniqueName = uniqid()+'.png'
					html += `<div class="col-md-6 position-relative" data-role="file" data-url="${image.URL}" data-name="${uniqueName}">
								<div class="position-absolute top-1 left-1 py-3">
									<input type="checkbox" class="form-check-input rounded-circle pointer width-25px height-25px" data-role="CHECK">
								</div>
								<img src="${image.URL}" class="w-100 h-100 img-fluid js_img_previewable " data-croppable="true" alt="">
							</div>`
					// append to weights_table_box_pptx_reports_files_list
					weights_table_box_pptx_reports_files_list.append(html)
					html = ''
					weights_table_box_pptx_files_panel__trigger.trigger('click')
				}
				
				dispatch_onNewAjaxContentLoaded()
			})

			return
		}
		
		if ( isImageFile(detail.files[0].path) )
		{
			WeightObject.append('file', detail.files[0])
			if ( weights_table_box_create_form__is_file_croppable_check.is(':checked') )
			{
				PreviewImageDialog({
					url: detail.files[0].path,
					file: detail.files[0],
					action: 'crop',
					// maxSize : [testImg.width, testImg.height],
				}, data =>
				{
					weights_table_box_create_form__file_preview.attr('src', data.url)
					console.log(data)
					WeightObject.set('file', data.file)
				})
			}
		}
	})


	const arm_circumferences_table_box_create_form__file_input = page_container.find('#arm_circumferences_table_box_create_form__file_input')
	const arm_circumferences_table_box_create_form__file_preview = page_container.find('#arm_circumferences_table_box_create_form__file_preview')

	var ArmCircumferenceObject = new FormData()

	// submit
	arm_circumferences_table_box_create_form.off('submit').on('submit', async e =>
	{
		e.preventDefault()

		// check if value not empty when type is "report"

		if ( arm_circumferences_table_box_create_form__weight_value_input.val() == '' )
		{
			CreateToast('PS', FUI_DISPLAY_LANG.views.messages.error_empty_input, '')
			return
		}
		
		ArmCircumferenceObject.append('clinicId', PatientObject.clinicId)
		ArmCircumferenceObject.append('clinicName', PatientObject.clinicName)
		ArmCircumferenceObject.append('patientId', PatientObject.patientId)
		ArmCircumferenceObject.append('patientName', PatientObject.patientName)
		ArmCircumferenceObject.append('value', arm_circumferences_table_box_create_form__weight_value_input.val())
		ArmCircumferenceObject.append('date', arm_circumferences_table_box_create_form__date_input.val())
		ArmCircumferenceObject.append('time', arm_circumferences_table_box_create_form__time_input.val())

		arm_circumferences_table_box_create_form__file_input.next().removeClass('d-none')
		arm_circumferences_table_box_create_form.find(':submit').addClass('disabled')

		// submit
		var res = await PATIENT_MODEL.arm_circumference_store(ArmCircumferenceObject, progress =>
		{
			var percent = progress.percentComplete.toFixed(2)
			// 
			arm_circumferences_table_box_create_form__file_input.next().find('.progress-bar').css('width', percent+'%').text(percent+'%')
		})

		CreateToast('PS', res.message, '')
		
		if ( res.code == 404 ) return

		arm_circumferences_table_box_create_form__reset()
		setup_arm_circumferences()
	})
	// select file
	arm_circumferences_table_box_create_form__file_input.off('file-selected').on('file-selected', e =>
	{
		var detail = e.originalEvent.detail

		if ( isImageFile(detail.files[0].path) )
		{
			ArmCircumferenceObject.append('file', detail.files[0])
			if ( arm_circumferences_table_box_create_form__is_file_croppable_check.is(':checked') )
			{
				PreviewImageDialog({
					url: detail.files[0].path,
					file: detail.files[0],
					action: 'crop',
					// maxSize : [testImg.width, testImg.height],
				}, data =>
				{
					arm_circumferences_table_box_create_form__file_preview.attr('src', data.url)
			
					ArmCircumferenceObject.set('file', data.file)
				})
			}
		}
	})

	// reset form
	function arm_circumferences_table_box_create_form__reset()
	{
		arm_circumferences_table_box_create_form__weight_value_input.val(null)
		arm_circumferences_table_box_create_form__file_input.val(null)
		arm_circumferences_table_box_create_form__file_preview.attr('src', imgPlaceholder())
		arm_circumferences_table_box_create_form__file_input.next().addClass('d-none')
		arm_circumferences_table_box_create_form.find(':submit').removeClass('disabled')
		ArmCircumferenceObject = new FormData()
	}

} 

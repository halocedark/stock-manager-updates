	// Preview report images in carousel
	async function weights_table_box_view_report_images__carousel(data)
	{
		const DEFAULT_CAROUSEL_TOTAL_PAGES = 7
		
        var printable_page_html_list = []
        var total_pages = (isNull(weights_table_box_view_report_images_in_slider_settings__images_number_input.val())) ? DEFAULT_CAROUSEL_TOTAL_PAGES : weights_table_box_view_report_images_in_slider_settings__images_number_input.val()

        var images = data.map((v,k) => {
			return {
				url: (v.file) ? v.file.url : '',
				timestamp: v.date+' '+v.time,

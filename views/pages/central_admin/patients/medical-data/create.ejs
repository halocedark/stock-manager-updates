// setup Reports
async function setup_patient_reports()
{
	var ReportObject = new FormData()

	// Actions menu
	patient_reports_box_more_actions__menu.off('click').on('click', e =>
	{
		var target = $(e.target)
		console.log(target[0])
	})
	// Table actions
	patient_reports_table_box_more_actions__menu.off('click').on('click', async e =>
	{
		var target = $(e.target)
		const parent = target.closest('[data-role="row"]')

		if ( target.data('role') == 'delete_patient_patient_reports' )
		{
			PromptConfirmDialog().then(async c =>
			{
				target.addClass('disabled')

				var res = await PATIENT_MODEL.report_batchDelete({
					collection: patient_reports_table_box_selectedRows()
				})

				target.removeClass('disabled')

				CreateToast('PS', res.message, '')

				if ( res.code == 404 ) return

				setup_patient_reports()
			})
		}
	})

	patient_reports_table_box_table_element.off('click').on('click', async e =>
	{
		var target = $(e.target)
		const parent = target.closest('[data-role="row"]')

		if ( target.data('role') == 'UPDATE' )
		{
			ReportObject.append('id', parent.data('id'))
			patient_reports_table_box__displayOne()
		}
	})

	// Display data
	// display all data in table
	patient_reports_table_box_displayData()
	async function patient_reports_table_box_displayData()
	{
		// total
		patient_reports_tab__patient_reports_box__total.text(0)
		//
		patient_reports_table_box_table_element.html(`<div class="col-md-12"><p class="placeholder-glow"><span class="placeholder col-12 bg-color-9"></span></p></div>`)

		var res = await PATIENT_MODEL.report_search({
			query: '',
			advanced: {
				patientId: PatientObject.patientId,
				type: 'report',
				orderBy: 'id',
				order: 'ASC',
			}
		})

		if ( res.code == 404 )
		{
			patient_reports_table_box_table_element.html(`
			<div class="col-md-12">
				<div class="border-top border-bottom px-3 py-3"><span class="fs-14 fw-400 text-color-17">${FUI_DISPLAY_LANG.views.messages.no_data_found}</span></div>
			</div>
				`)
			return
		}

		var data = res.data
		var html = ''
		// total
		patient_reports_tab__patient_reports_box__total.text(data.length)

		$.each(data, (k,v) =>
		{

			html += `<div class="col-md-12 px-0">

						<div class="border-top px-3 py-3 rounded-3" data-role="row" data-id="${v.id}">

							<div class="row">

								<div class="col-md-2 d-flex align-items-center px-0">
									<input type="checkbox" class="form-check-input rounded-circle pointer width-25px height-25px" data-role="CHECK">
								</div>

								<div class="col-md px-0">
									<div class="fs-14 fw-300 mb-1">${ strSnippet(v.body) }</div>
									<div class="fs-14 fw-400 text-color-17">${v.created_at_diff.auto} (${v.date_format})</div>
									<div class="fs-14 fw-400 text-color-17">${FUI_DISPLAY_LANG.views.pages.global.created_by} (${v.employee_name})</div>
								</div>

								<div class="col-md-2 d-flex align-items-center">
									
									<a href="#" class="fs-14 js_panel_toggler_button" data-target="#patient_reports_table_box_table_row_details_panel_${v.id}">${FUI_DISPLAY_LANG.views.pages.global.details_button}</a>

								</div>

							</div>

							<div class="d-none bg-color-26 rounded mt-3 px-3 py-3" id="patient_reports_table_box_table_row_details_panel_${v.id}">

								<div class="has-text-aligned-locale-inverted">
									<div class="close-button-sm js_panel_close_button" data-target="#patient_reports_table_box_table_row_details_panel_${v.id}">
										<i class="xfb xfb-close-sm-thin"></i>
									</div>
								</div>

								<div class="d-flex mb-2">
									
									
									<div data-perm="update_patient_reports">
										<a href="#" data-role="UPDATE">${FUI_DISPLAY_LANG.views.pages.global.actions.update}</a>	
									</div>
									
								</div>

								<div class="py-2 px-2 rounded bg-white">
									${v.body}
								</div>

							</div>

						</div>

					</div>PAG_SEP`
		})

		new Paginator(patient_reports_table_box_table_element, html.split('PAG_SEP'), {
            perPage: 8,
            position: 'bottom',
            links: {
                visible: true
            }
        })
	}
	// selected rows
	function patient_reports_table_box_selectedRows()
	{
		var list = []
		var items = patient_reports_table_box_table_element.find('[data-role="row"]')
		for (var i = 0; i < items.length; i++) 
		{
			var parent = $(items[i])
			var check = parent.find('[data-role="CHECK"]')
			if ( check.is(':checked') )
			{
				list.push({
					id: parent.data('id'),
				})
			}
		}

		return list
	}

	// Insert data
	const patient_reports_table_box_pptx_reports_files_list = page_container.find('#patient_reports_table_box_pptx_reports_files_list')
	const patient_reports_table_box_pptx_files_panel__trigger = page_container.find('#patient_reports_table_box_pptx_files_panel__trigger')

	const patient_reports_table_box_create_form = page_container.find('#patient_reports_table_box_create_form')
	const patient_reports_table_box_create_form__body_input = page_container.find('#patient_reports_table_box_create_form__body_input')
	const patient_reports_table_box_create_form__date_input = page_container.find('#patient_reports_table_box_create_form__date_input')
	const patient_reports_table_box_create_form__time_input = page_container.find('#patient_reports_table_box_create_form__time_input')

	// submit
	patient_reports_table_box_create_form.off('submit').on('submit', async e =>
	{
		e.preventDefault()

		// check if value not empty when type is "report"

		if ( patient_reports_table_box_create_form__body_input.val() == '' )
		{
			CreateToast('PS', FUI_DISPLAY_LANG.views.messages.error_empty_input, '')
			return
		}
		
		ReportObject.append('clinicId', PatientObject.clinicId)
		ReportObject.append('clinicName', PatientObject.clinicName)
		ReportObject.append('patientId', PatientObject.patientId)
		ReportObject.append('patientName', PatientObject.patientName)
		ReportObject.append('body', patient_reports_table_box_create_form__body_input.val())
		ReportObject.append('date', patient_reports_table_box_create_form__date_input.val())
		ReportObject.append('time', patient_reports_table_box_create_form__time_input.val())

		patient_reports_table_box_create_form.find(':submit').addClass('disabled')
		// update
		if ( ReportObject.get('id') )
		{

			var res = await PATIENT_MODEL.report_update(ReportObject)

			CreateToast('PS', res.message, '')
			
			if ( res.code == 404 ) return

			patient_reports_table_box_create_form__reset()
			setup_patient_reports()

			return
		}
		// store
		var res = await PATIENT_MODEL.report_store(ReportObject)

		CreateToast('PS', res.message, '')
		
		if ( res.code == 404 ) return

		patient_reports_table_box_create_form__reset()
		setup_patient_reports()
	})

	// reset form
	function patient_reports_table_box_create_form__reset()
	{
		patient_reports_table_box_create_form__body_input.val(null)
		patient_reports_table_box_create_form.find(':submit').removeClass('disabled')
		ReportObject = new FormData()
	}
	// display info
	async function patient_reports_table_box__displayOne()
	{
		if ( !ReportObject.get('id') ) return

		var res = await PATIENT_MODEL.report_show({
			id: ReportObject.get('id')
		})

		if ( res.code == 404 )
		{
			CreateToast('PS', res.message, '')
			return
		}

		const data = res.data

		// ReportObject.append('body', data.body)
		// ReportObject.append('date', data.date)
		// ReportObject.append('time', data.time)

		patient_reports_table_box_create_form__body_input.val(data.body)
		patient_reports_table_box_create_form__date_input.val(data.date)
		patient_reports_table_box_create_form__time_input.val(data.time)
	}
} 


})


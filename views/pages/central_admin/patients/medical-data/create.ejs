// setup weights
async function setup_weights()
{

	// Actions menu
	weights_box_more_actions__menu.off('click').on('click', e =>
	{
		var target = $(e.target)
		console.log(target[0])
	})
	// Table actions
	weights_table_box_more_actions__menu.off('click').on('click', e =>
	{
		var target = $(e.target)
		
		if ( target.data('role') == 'delete_patient_weights' )
		{
			PromptConfirmDialog().then(async c =>
			{
				target.addClass('disabled')

				var res = await PATIENT_MODEL.weight_batchDelete({
					collection: weights_table_box_selectedRows()
				})

				target.removeClass('disabled')

				CreateToast('PS', res.message, '')

				if ( res.code == 404 ) return

				setup_weights()
			})
		}
	})

	// Display data
	// Display first weight
	weights_box__firstRecord()
	async function weights_box__firstRecord()
	{
		weights_box_first_weight_value.text(0)

		var res = await PATIENT_MODEL.weight_search({
			query: '',
			advanced: {
				patientId: PatientObject.patientId,
				type: 'report',
				select: {
					search: "SELECT * FROM",
					replace: "SELECT value FROM",
				},
				orderBy: 'id',
				order: 'ASC',
				offset: 0,
				limit: 1
			}
		})

		if ( res.code == 404 ) return
		if ( !res.data[0] ) return

		weights_box_first_weight_value.text(res.data[0].value)
	}
	// Display last weight
	weights_box__lastRecord()
	async function weights_box__lastRecord()
	{
		weights_box_last_weight_value.text(0)

		var res = await PATIENT_MODEL.weight_search({
			query: '',
			advanced: {
				patientId: PatientObject.patientId,
				type: 'report',
				select: {
					search: "SELECT * FROM",
					replace: "SELECT value FROM",
				},
				orderBy: 'id',
				order: 'DESC',
				offset: 0,
				limit: 1
			}
		})

		if ( res.code == 404 ) return
		if ( !res.data[0] ) return

		weights_box_last_weight_value.text(res.data[0].value)
	}
	// display all time data in chart
	weights_box__chartData()
	async function weights_box__chartData()
	{
		var res = await PATIENT_MODEL.weight_search({
			query: '',
			advanced: {
				patientId: PatientObject.patientId,
				type: 'report',
				select: {
					search: "SELECT * FROM",
					replace: "SELECT value as total, date FROM",
				},
				groupBy: 'date'
			}
		})

		weights_box_chart.parent().addClass('d-none')
		if ( res.code == 404 ) return
		if ( !res.data[0] ) return

		var data = res.data
        var labels = []
        var values = []
       
        $.each(data, (k,v) =>
        {
            labels.push( v.date )
            values.push( v.total )
        })

        weights_box_chart.parent().removeClass('d-none')
    
        create_weights_box_chart({
            data: {
                labels: labels,
                datasets: [
                    {
                        label: FUI_DISPLAY_LANG.views.pages.global.statistics.text34,
                        data: values,
                        borderColor: 'rgba(0, 255, 153, 1)',
                        backgroundColor: 'rgba(0, 255, 153, 0.2)',
                        borderWidth: 1,
                        borderRadius: 3,
                        borderSkipped: false,
                    }
                ]
            }
        })
	}
	// create chart
    function create_weights_box_chart(options = {})
    {
        const ctx = weights_box_chart[0].getContext('2d');

        const config = {
            type: 'line',
            data: options.data,
            options: {
                responsive: true,
                height: 500,
                plugins: {
                    legend: {
                        position: 'top',
                    },
                    title: {
                        display: true,
                        text: FUI_DISPLAY_LANG.views.pages.global.statistics.text28
                    }
                }
            },
        }

        if ( weights_box_chartjs ) weights_box_chartjs.destroy()

        weights_box_chartjs = new Chart(ctx, config)
                
    }

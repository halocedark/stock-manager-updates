					id: parent.data('id'),
				})
			}
		}

		return list
	}

	// Insert data
	const arm_circumferences_table_box_pptx_reports_files_list = page_container.find('#arm_circumferences_table_box_pptx_reports_files_list')
	const arm_circumferences_table_box_pptx_files_panel__trigger = page_container.find('#arm_circumferences_table_box_pptx_files_panel__trigger')

	const arm_circumferences_table_box_create_form = page_container.find('#arm_circumferences_table_box_create_form')
	const arm_circumferences_table_box_create_form__weight_value_input = page_container.find('#arm_circumferences_table_box_create_form__weight_value_input')
	const arm_circumferences_table_box_create_form__date_input = page_container.find('#arm_circumferences_table_box_create_form__date_input')
	const arm_circumferences_table_box_create_form__time_input = page_container.find('#arm_circumferences_table_box_create_form__time_input')

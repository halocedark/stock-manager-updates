	// selected rows
	function pelvic_circumferences_table_box_selectedRows()
	{
		var list = []
		var items = pelvic_circumferences_table_box_table_element.find('[data-role="row"]')
		for (var i = 0; i < items.length; i++) 
		{
			var parent = $(items[i])

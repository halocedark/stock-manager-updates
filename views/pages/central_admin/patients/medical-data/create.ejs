		partial_body_analisyss_table_box_create_form__select_files_input.val(null)
		partial_body_analisyss_table_box_create_form__select_files_input.next().addClass('d-none')
		partial_body_analisyss_table_box_create_form.find(':submit').removeClass('disabled')
		PartialBodyAnalysisResultObject = new FormData()
	}
	// display images
	async function partial_body_analisyss_table_box__displayImages(files)
	{
		partial_body_analisyss_table_box_image_files_list.html('')

		var html = ''
		SectionLoader(partial_body_analisyss_table_box_create_form__select_files_input.closest('.js_select_file_component'))

		fileToDataUrlWorker(files).then(dataUrls =>
		{
			SectionLoader(partial_body_analisyss_table_box_create_form__select_files_input.closest('.js_select_file_component'), '')
			for (let i = 0; i < dataUrls.length; i++) 
			{
				const file = dataUrls[i]
				var type = extractExtensionFromDataUrl(file.url)

				html += `<div class="col-md-4 position-relative ${ (!isImageFile(file.url)) ? 'd-none' : '' }" data-role="row" data-name="${file.name}" data-url="${file.url}" data-type="${type}">
							<div class="position-absolute top-1 left-1 py-3">
								<input type="checkbox" class="form-check-input rounded-circle pointer width-25px height-25px" data-role="CHECK">
							</div>
							<img src="${file.url}" class="w-100 h-100 img-fluid js_img_previewable " data-croppable="true" alt="">
						</div>`
				
			}
			
			partial_body_analisyss_table_box_image_files_list.html(html)
			// dispatch_onNewAjaxContentLoaded
			dispatch_onNewAjaxContentLoaded()
			partial_body_analisyss_table_box_image_files_panel__trigger.trigger('click')
		})

	}
	// selected images
	function partial_body_analisyss_table_box__selectedImages()
	{
		var list = []
		partial_body_analisyss_table_box_image_files_list.find('[data-role="row"]').map((k,v) =>
		{
			const parent = $(v)
			const check = parent.find('[data-role="CHECK"]')

			if ( check.is(':checked') )

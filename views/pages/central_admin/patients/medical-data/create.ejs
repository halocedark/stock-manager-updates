		pelvic_circumferences_table_box_create_form.find(':submit').addClass('disabled')

		// submit
		var res = await PATIENT_MODEL.pelvic_circumference_store(PelvicCircumferenceObject, progress =>
		{
			var percent = progress.percentComplete.toFixed(2)
			// 
			pelvic_circumferences_table_box_create_form__file_input.next().find('.progress-bar').css('width', percent+'%').text(percent+'%')
		})

		CreateToast('PS', res.message, '')
		
		if ( res.code == 404 ) return

		pelvic_circumferences_table_box_create_form__reset()
		setup_pelvic_circumferences()
	})
	// select file
	pelvic_circumferences_table_box_create_form__file_input.off('file-selected').on('file-selected', e =>
	{
		var detail = e.originalEvent.detail

		if ( isImageFile(detail.files[0].path) )
		{
			PelvicCircumferenceObject.append('file', detail.files[0])
			if ( pelvic_circumferences_table_box_create_form__is_file_croppable_check.is(':checked') )
			{
				PreviewImageDialog({
					url: detail.files[0].path,
					file: detail.files[0],
					action: 'crop',
					// maxSize : [testImg.width, testImg.height],
				}, data =>

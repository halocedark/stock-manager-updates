<script>

$(function()
{

var page_container = $('#allFundboxesContainer');
if ( !page_container[0] ) return

var ERROR_BOX = page_container.find('#ERROR_BOX');
var deleteSelectedBTN = page_container.find('#deleteSelectedBTN');
var pagination = page_container.find('#pagination');
var table_element = page_container.find('#table_element');

var owner_type_select = page_container.find('#owner_type_select')

var search_btn = page_container.find('#search_btn');
var search_input = page_container.find('#search_input');
var date_start_input = page_container.find('#date_start_input');
var date_end_input = page_container.find('#date_end_input');

var fundboxes_total = page_container.find('#fundboxes_total');
var fundboxes_extra_data_list = page_container.find('#fundboxes_extra_data_list');

var wrapper__1 = page_container.find('#wrapper__1')
var wrapper__2 = page_container.find('#wrapper__2')

var BACK_TO_MAIN_UI_BTN = page_container.find('[data-role="BACK_TO_MAIN_UI_BTN"]')

var FundBoxObject = {}

// back to main ui
BACK_TO_MAIN_UI_BTN.on('click', e =>
{
    BACK_TO_MAIN_UI_BTN.closest('.WRAPPER').slideUp(200).siblings('.WRAPPER').slideDown(200)
})
// table_element click
table_element.on('click', async e =>
{
    var target = $(e.target);
    if ( target.data('role') == 'UPDATE' )
    {
        var parent = target.closest('[data-role="row"]')
        var id = parent.data('id');

        var res = await FUNDBOX_MODEL.show({
            id: id
        })

        goToPage('create-fundboxes', res.data)
        //rebindEvents();
    }
    else if ( target.data('role') == 'row' )
    {
        var parent = target.closest('[data-role="row"]')
        var id = parent.data('id');

        var res = await FUNDBOX_MODEL.show({
            id: id
        })

        FundBoxObject = res.data
        displayInfo()

    }
});
// delete
deleteSelectedBTN.on('click', e =>
{
    PromptConfirmDialog().then(c =>
    {
        SectionLoader(table_element);
        FUNDBOX_MODEL.batchDelete( {
            collection: getSelectedRows()
        } ).then(response =>
        {
            // hide loader
            SectionLoader(table_element, '');
            if ( response.code == 404 )
            {
                ERROR_BOX.show(0).delay(7*1000).hide(0)
                .find('#text').text(response.message);
                return;
            }
            ERROR_BOX.show(0).delay(7*1000).hide(0)
            .find('#text').text(response.message);
            //
            displayAll({
                method_type: 'search',
                query: search_input.val(),
            })
        });
    });
});
// search
search_btn.on('click', e =>
{
    displayAll({
        method_type: 'search',
        query: search_input.val(),
        advanced: {
            start: date_start_input.val(),
            end: date_end_input.val(),
        }
    })
})
search_input.on('keyup', async e =>
{
    displayAll({
        method_type: 'search',
        query: search_input.val(),
    })
});
// filter
owner_type_select.on('change', e =>
{
    var val = owner_type_select.find(':selected').val()

    displayAll({
        method_type: 'filter',
        filter: val,
    })
})
// display all
displayAll({
    method_type: 'search',
    query: search_input.val(),
})
async function displayAll(params)
{
    // display loader
    table_element.html('')
    SectionLoader(table_element)
    fundboxes_extra_data_list.html('')
    if ( params.method_type == 'search' )
        var res = await FUNDBOX_MODEL.search(params)
    if ( params.method_type == 'filter' )
        var res = await FUNDBOX_MODEL.filter(params)
    // hide loader
    SectionLoader(table_element, '')
    // clear html
    if ( res.code == 404 ) return;

    var data = res.data
    var html = ''
    var fundboxes_total_amount = 0
    var max_amount_data = {}
    var max = 0
    $.each(data, (k,v) =>
    {
        var total = parseFloat(v.total)
        // max amount
        if ( total >= max )
        {
            max = total
            max_amount_data = v
        }
        // total
        fundboxes_total_amount += total

        html+= `<div class="col-lg-3 col-md-6 col-sm-12">
                    <div class="card hover-shadow h-100" data-role="row" data-id="${v.id}" style="cursor: pointer;">
                        <div class="pt-2 px-2">
                            <div class="form-check">
                                <input class="form-check-input" type="checkbox" data-role="CHECK" value="" id="flexCheckDefault_${v.id}">
                                <label class="form-check-label" for="flexCheckDefault_${v.id}">
                                    ${FUI_DISPLAY_LANG.views.pages.global.select}
                                </label>
                            </div>
                        </div>
                        <div class="card-body no-pointer">
                            <div class="h5">${FUI_DISPLAY_LANG.views.pages.global.label34}: ${v.owner_name}</div>
                            <span class="text-02">${FUI_DISPLAY_LANG.views.pages.global.label36}:  ${FUI_DISPLAY_LANG.views.pages.global.attributes[v.owner_type]}</span>
                            <div class="text-success fw-500 mt-2">${FUI_DISPLAY_LANG.views.pages.global.total_amount}: ${ moneyFormat(v.total) }</div>
                            <div class="mt-2">
                                <span>(<span class="text-success">${ (v.in_amount > 0) ? '+'+ moneyFormat(v.in_amount) : '0.00' }</span>)</span>

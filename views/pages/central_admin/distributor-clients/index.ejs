// selected rows
function selectedRows()
{
    var list = [];
    var items = distributor_clients_table_element.find('[data-role="row"]');
    for (var i = 0; i < items.length; i++) 
    {
        var parent = $(items[i])
        var check = parent.find('[data-role="CHECK"]')
        if ( check.is(':checked') )
        {
            list.push({
                id: check.data('id')
            });
        }
    }

    return list;
}


    async function displayAllDeptActions(params)
    {
        // total_depts
        total_depts.text(moneyFormat(ClientObject.total_depts)).data('total', ClientObject.total_depts).attr('data-total', ClientObject.total_depts)
        //
        dept_actions_extra_data_list.html('')

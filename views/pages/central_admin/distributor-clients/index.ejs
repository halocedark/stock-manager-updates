<script>

$(function()
{

var page_container = $('#view_distributors_clients_container');
if ( page_container[0] == undefined )
    return;

var ERROR_BOX = page_container.find('#ERROR_BOX');

var deleteSelectedBTN = page_container.find('#deleteSelectedBTN');
var distributor_clients_table_element_pagination = page_container.find('#distributor_clients_table_element_pagination');
var distributor_clients_table_element = page_container.find('#distributor_clients_table_element');

var select_distributors_template_distributor_select = page_container.find('#select_distributors_template_distributor_select');

var distributor_clients_search_input = page_container.find('#distributor_clients_search_input');
var distributor_clients_search_btn = page_container.find('#distributor_clients_search_btn');
var distributor_clients_date_start_input = page_container.find('#distributor_clients_date_start_input');
var distributor_clients_date_end_input = page_container.find('#distributor_clients_date_end_input');

var clients_total = page_container.find('#clients_total');
var clients_extra_data_list = page_container.find('#clients_extra_data_list');
var all_clients_mapElement = page_container.find('#all_clients_mapElement');

var distributor_clients_location_map = page_container.find('#distributor_clients_location_map')

var total_depts = page_container.find('#total_depts');

var single_distributor_client_table_element = page_container.find('#single_distributor_client_table_element');

var BACK_TO_MAIN_UI_BTN = page_container.find('[data-role="BACK_TO_MAIN_UI_BTN"]');

var wrapper01 = page_container.find('#wrapper01');
var wrapper02 = page_container.find('#wrapper02');
var wrapper03 = page_container.find('#wrapper03');

// back to main ui
BACK_TO_MAIN_UI_BTN.on('click', e =>
{
    wrapper01.slideDown(200).siblings('.WRAPPER').slideUp(200);
});
var ClientObject = {};
// distributor_clients_table_element click
distributor_clients_table_element.on('click', async e =>
{
    var target = $(e.target);
    if ( target.data('role') == 'UPDATE' )
    {
        var parent = target.closest('[data-role="row"]')
        var id = parent.data('id');
        goToPage('create-distributor-clients', {
            id: id
        })
    }
    else if ( target.data('role') == 'row' )
    {
        var parent = target.closest('[data-role="row"]')
        var res = await EMPLOYEE_MODEL.distributor_client_show({
            id: parent.data('id')
        })
        ClientObject = res.data
        displayInfo();
    }
});
// delete selected
deleteSelectedBTN.on('click', async e =>
{
    PromptConfirmDialog().then(async c =>
    {
        // display loader
        if ( FUI_DISPLAY_LANG.lang == 'ar' )
            TopLoader("حذف البيانات...");
        else if ( FUI_DISPLAY_LANG.lang == 'fr' )
            TopLoader("Suprimmer les données...");

        var response = await EMPLOYEE_MODEL.distributor_client_batchDelete({
            collection: selectedRows()
        });
        // hide loader
        TopLoader('', false);
        if ( response.code == 404 )
        {
            ERROR_BOX.show(0).delay(7*1000).hide(0).find('#text').text(response.message);
            return;
        }
        ERROR_BOX.show(0).delay(7*1000).hide(0).find('#text').text(response.message);
        //
        displayAll();
    });
});
// search
distributor_clients_search_btn.on('click', async e =>
{
    displayAll({
        method_type: 'distributor_client_local_search',
        distributor_id: select_distributors_template_distributor_select.find(':selected').val(),
        query: distributor_clients_search_input.val(),
        advanced: {
            start: distributor_clients_date_start_input.val(),
            end: distributor_clients_date_end_input.val(),
        }
    })
})
distributor_clients_search_input.on('keyup', async e =>
{
    displayAll({
        method_type: 'distributor_client_local_search',
        distributor_id: select_distributors_template_distributor_select.find(':selected').val(),
        query: distributor_clients_search_input.val(),
    })
})
select_distributors_template_distributor_select.on('change', async e =>
{
    displayAll({
        method_type: 'distributor_client_local_search',
        distributor_id: select_distributors_template_distributor_select.find(':selected').val(),
        query: distributor_clients_search_input.val(),
    })
})
// 
$(document).off('disctributor-selected').on('distributor-selected', e =>
{
    var detail = e.originalEvent.detail

    displayAll({
        method_type: 'distributor_client_local_search',
        distributor_id: detail.distributor.id,
        query: distributor_clients_search_input.val(),
    })
})

async function displayAll(params)
{
    // total
    clients_total.text(0)

    clients_extra_data_list.html('')
    SectionLoader( distributor_clients_table_element );
    // map
    all_clients_mapElement.removeClass('height-500px')
    //
    distributor_clients_table_element.html('');
    if ( params.method_type == 'distributor_client_local_search' )
        var res = await EMPLOYEE_MODEL.distributor_client_local_search(params)

    SectionLoader( distributor_clients_table_element, '' );
    if ( res.code == 404 )
        return;

    var data = res.data;
    var html = '';
    var last_data = firstArrayElement(data)
    // total
    clients_total.text(data.length)
    var points = []
    $.each(data, (k,v) =>
    {
        if ( v.coordinates )
        {
            points.push({
                coordinates: convertCoordinates(v.coordinates, 'object')
            })
        }
        html += `<div class="col-lg-3 col-md-6 col-sm-12">
                    <div class="card hover-shadow h-100" data-role="row" data-id="${v.pharmacy_id}" style="cursor: pointer;">
                        <div class="form-check border-bottom p-1">
                            <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault_${v.pharmacy_id}" data-role="CHECK">
                            <label class="form-check-label" for="flexCheckDefault_${v.pharmacy_id}">
                                ${FUI_DISPLAY_LANG.views.pages.global.select}
                            </label>
                        </div>
                        <div class="card-body no-pointer">
                            <div class="h5">${v.name}</div>
                            <span class="text-02">${v.phone}</span>
                            <span class="text-04 text-muted">(${v.wilaya}-${v.baladia})</span>
                            <div class="text-primary">${ (v.coordinates) ?? '' }</div>
                            <div>${ FUI_DISPLAY_LANG.views.pages.global.text21 }: <span class="text-success">${moneyFormat(v.total_depts)}</span></div>
                        </div>
                        <div class="pb-2">
                            <a href="#" data-role="UPDATE" data-perm="update_distributor_clients" class="mx-2 text-dark">
                                <i class="fas fa-edit no-pointer"></i>
                            </a>
                        </div>
                    </div>
                </div>PAG_SEP`;	
    });
    // add html
    var options = {
        data: html.split('PAG_SEP')
    };
    new SmoothPagination(distributor_clients_table_element_pagination, distributor_clients_table_element, options);
    // extra data
    clients_extra_data_list.html(`
        <li class="list-group-item">
            <span class="fw-500">${ FUI_DISPLAY_LANG.views.pages.global.text36 }: </span>
            <span>${FUI_DISPLAY_LANG.views.pages.global.for_} </span>
            <span>${last_data.name}, </span>
            <span class="text-primary">${last_data.created_at_date} ${last_data.created_at_time}</span> 
        </li>
        <li class="list-group-item">
            <span class="fw-500">RC: </span>
            <span>${last_data.RC} </span>
        </li>
        <li class="list-group-item">
            <span class="fw-500">NIF: </span>
            <span>${last_data.NIF} </span>
        </li>
        <li class="list-group-item">
            <span class="fw-500">NIS: </span>
            <span>${last_data.NIS} </span>
        </li>
        <li class="list-group-item">
            <span class="fw-500">${FUI_DISPLAY_LANG.views.pages.global.text48}: </span>
            <span>${ (last_data.address) ?? '' } ${ (last_data.baladia) ? ' - '+last_data.baladia : '' } ${ (last_data.wilaya) ? ' - '+last_data.wilaya : '' }</span>
        </li>
    `)
    // show in map
    if ( points.length > 0 )
    {
        all_clients_mapElement.addClass('height-500px')
        batchShowInMap(all_clients_mapElement[0], points)
    }
}
// display info
async function displayInfo()
{
    if ( !ClientObject.pharmacy_id ) return
    // display loader
    SectionLoader(wrapper02);
    wrapper02.slideDown(200).siblings('.WRAPPER').slideUp(200);
    var response = await EMPLOYEE_MODEL.distributor_client_show({
        id: ClientObject.pharmacy_id,
    });
    // hide loader
    SectionLoader(wrapper02, '');
    // clear html
    single_distributor_client_table_element.find('#content').html('');
    if ( response.code == 404 )
        return;

    var data = response.data;
    var html = '';
    // display location in map
    distributor_clients_location_map.html('')
    if ( data.coordinates )
    {
        showInMap( distributor_clients_location_map[0], {
            coordinates: convertCoordinates(data.coordinates, 'object'),
            zoom: 11,
        })
    }
    else
    {
        showInMap( distributor_clients_location_map[0])
    }

    html = `<div class="">
                <div class="row gx-4 gy-3 mb-3">
                    <div class="row gx-4 gy-3">
                        <div class="col-md-6 col-sm-12">
                            <div class="d-inline-flex flex-align-center w-100 h-100">
                                <div>
                                    <div class="h5">${data.name}</div>
                                    <div class="text-02">${data.phone}</div>
                                </div>	
                            </div>
                        </div>
                    </div>
                </div>
                <div class="mb-3 border-bottom">
                    <div class="mb-2">
                        <div class="text-01 mb-3">${ FUI_DISPLAY_LANG.views.pages.global.text19 }</div>
                        <div class="text-04 mb-2">${data.wilaya}-${data.baladia}</div>
                        <div class="text-04 mb-2">${data.address}</div>
                        <div class="mb-2 text-primary">${ (data.coordinates) ?? '' }</div>
                    </div>
                </div>
                <div class="mb-3 border-bottom">
                    <div class="mb-2">
                        <div class="text-01 mb-3">${ FUI_DISPLAY_LANG.views.pages.global.text20 }</div>
                        <div class="text-04 mb-2">${ (data.RC) ?? '' }</div>
                        <div class="text-04 mb-2">${ (data.NIF) ?? '' }</div>
                        <div class="text-04 mb-2">${ (data.NIS) ?? '' }</div>
                    </div>
                </div>
            </div>`;	
    // add html
    single_distributor_client_table_element.find('#content').html(html);

    // Products Invoices
    products_invoices = page_container.find('#products_invoices')
    products_invoices_search_btn = products_invoices.find('#products_invoices_search_btn')
    products_invoices_search_input = products_invoices.find('#products_invoices_search_input')
    products_invoices_date_start_input = products_invoices.find('#products_invoices_date_start_input')

<script>

    $(async function () {

        var page_container = $('#centralAdministrationEmployeesPermissionsContainer');
        if (!page_container[0])
            return;

        const select_all_permissions_check = page_container.find('#select_all_permissions_check')

        var clinicSelect = page_container.find('#clinicSelect');
        var employeeTypesSelect = page_container.find('#employeeTypesSelect');

        var permissions_accordion1 = page_container.find('#permissions_accordion1');
        var submit_btn = page_container.find('#submit_btn');

        submit_btn.off('click');
        submit_btn.on('click', async e => {
            SectionLoader(permissions_accordion1);

            var response = await EMPLOYEE_MODEL.type_permission_setBatch({
                administration_id: 0,
                employee_type_id: employeeTypesSelect.find(':selected').val(),
                permissions: checkedPerms()
            });

            SectionLoader(permissions_accordion1, '');
            if (response.code == 404) {
                if (FUI_DISPLAY_LANG.lang == 'ar')
                    CreateToast('', response.message, '');
                else if (FUI_DISPLAY_LANG.lang == 'fr')
                    CreateToast('Notification', response.message, 'maintenant');
                return;
            }

            if (FUI_DISPLAY_LANG.lang == 'ar')
                CreateToast('', response.message, '');
            else if (FUI_DISPLAY_LANG.lang == 'fr')
                CreateToast('Notification', response.message, 'maintenant');
        });

        // select employee type
        employeeTypesSelect.off('change');
        employeeTypesSelect.on('change', e => {
            selectEmployeeTypePermissions();
        });
        // select all
        select_all_permissions_check.on('click', e =>
        {
            selectAllPermissions()
        })
        // checked permissions
        function checkedPerms() {
            var granted = [];
            var denied = [];
            var checks = permissions_accordion1.find('[data-role="permission"]');
            for (let i = 0; i < checks.length; i++) {
                const check = $(checks[i]);
                if (check.is(':checked'))
                    granted.push(check.attr('id'));
                else
                    denied.push(check.attr('id'));
            }

            return {
                granted: granted,
                denied: denied,
            }
        }
        // select employee type permissions
        async function selectEmployeeTypePermissions() {
            SectionLoader(permissions_accordion1)
            var response = await EMPLOYEE_MODEL.type_permission_index({
                administration_id: 0,
                employee_type_id: employeeTypesSelect.find(':selected').val()
            });
            SectionLoader(permissions_accordion1, '')
            if (response.code == 404)
                return;

            var permissions = response.data;
            console.log(permissions);
            var checks = permissions_accordion1.find('[data-role="permission"]');
            for (let i = 0; i < checks.length; i++) {
                const check = $(checks[i]);
                // granted
                for (let j = 0; j < permissions.granted.length; j++) {
                    const perm = permissions.granted[j];
                    if (check.attr('id') == perm)
                        check.prop('checked', true);
                }
                // denied
                for (let j = 0; j < permissions.denied.length; j++) {
                    const perm = permissions.denied[j];
                    if (check.attr('id') == perm)
                        check.prop('checked', false);
                }
            }
        }
        // select all permissions
        function selectAllPermissions()
        {
            const checks = permissions_accordion1.find('[data-role="permission"]')

            checks.each((i,v) =>
            {
                const check = $(v)
                const parent = check.closest('.accordion-item')

                if ( !parent.hasClass('d-none') )
                    check.prop('checked', select_all_permissions_check.prop('checked'))

            })
        }
    })

</script>
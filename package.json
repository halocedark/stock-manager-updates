{
  "name": "stock_manager",
  "version": "0.1.0",
  "productName": "Stock Manager",
  "description": "",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/halocedark/stock-manager-updates.git"
  },
  "build": {
    "asar": true,
    "afterPack": "afterPack.js",
    "publish": [
      {
        "provider": "github",
        "owner": "halocedark",
        "repo": "stock-manager-updates"
      }
    ],
    "appId": "com.holoolaz.stock_manager",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "assets/ico/favicon.ico",
      "requestedExecutionLevel": "highestAvailable"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "start": "electron .",
    "publish": "electron-builder -p always",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "asarmor": "^2.0.0",
    "axios": "^0.24.0",
    "child_process": "^1.0.2",
    "cloudmersive-convert-api-client": "^2.7.4",
    "date-and-time": "^2.4.0",
    "ejs-electron": "^2.1.1",
    "electron-is-dev": "^2.0.0",
    "electron-root-path": "^1.0.16",
    "electron-updater": "^4.6.5",
    "ini": "^2.0.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "node-xlsx": "^0.21.0",
    "papaparse": "^5.3.2",
    "qrcode": "^1.5.0",
    "qs": "^6.11.2",
    "read-excel-file": "^5.5.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "url-file-size": "^1.0.5-1",
    "uuid": "^8.3.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "electron": "^16.0.9",
    "electron-builder": "^22.14.13"
  }
}
